provider "aws" {
  region  = "var.aws_region
}
resource "aws_instance" "testinstance" {
  ami           = "ami-052cef05d01020f1d"
  instance_type = "t2.micro"
  key_name      = "my key"
  tags          = {
    Name = "testinstance"
  }
  }
variable "aws_region" {
  description = "create things"
  default     = "ap-south-1"
}
